		 weekHeader: 'Sm',
		 dateFormat: 'dd/mm/yy',
		 firstDay: 1,
		 isRTL: false,
		 showMonthAfterYear: false,
		 yearSuffix: ''
	 };
	 $.datepicker.setDefaults($.datepicker.regional['es']);
}
function removeNulls(){
	$("td").each(function(){
	    if($(this).text() == 'null'){
	    	$(this).text("");
	    }
	});
	$("span").each(function(){
	    if($(this).text() == 'null'){
	    	$(this).text("");
	    }
	});
}
